{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "eol-last": 0,
    "react/jsx-no-bind": "off",
    "import/no-extraneous-dependencies": "off",
    "react/jsx-no-undef": "error",
    "linebreak-style": ["off"]
  }
}